        "postcss-load-config": "^3.1.3",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.6.0",
        "svelte": "^3.46.4",
        "svelte-check": "^2.4.5",
        "svelte-inview": "^3.0.0",
        "svelte-jester": "^2.3.2",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.10.3",
        "svelte2tsx": "^0.5.5",
        "tailwindcss": "^3.0.23",
        "ts-jest": "^27.1.3",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5"
