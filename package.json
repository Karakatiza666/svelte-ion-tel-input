    "version": "0.5.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gyurielf/svelte-tel-input.git"
    },
    "homepage": "https://github.com/gyurielf/svelte-tel-input#readme",
    "keywords": [
        "svelte",
        "tel input",
        "phone",
        "phone input",
        "intl",
        "intl tel input"
    ],
    "engines": {
        "npm": ">= 7",
        "yarn": ">=2",
        "node": ">= 16"
